#include<iostream>
using namespace std;

class A2{
	int countA2;
	//friend void getA2(A2 obj1);  // friend function declaration
	public:
		
		A2()
		{
			countA2=15;
		}
		friend void getA2(A2 obj1);
}; 

void getA2(A2 obj1)
{
	cout<<"count of A2="<<obj1.countA2;
}

int main()
{
	A2 obj;
	getA2(obj);

}
